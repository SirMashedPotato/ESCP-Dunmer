<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Ashlander Tribe -->
  
  <RulePackDef>
    <defName>ESCP_NamerFactionAshlanderTribe</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[FamilyList] Tribe</li>
      </rulesStrings>
	  <rulesFiles>
        <li>FamilyList->ESCP_AshlanderNames/AshlanderFamily</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ESCP_NamerSettlementAshlanderTribe</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[FamilyList] Camp</li>
      </rulesStrings>
	  <rulesFiles>
        <li>FamilyList->ESCP_AshlanderNames/AshlanderFamily</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <!-- Great House -->
  
  <RulePackDef>
    <defName>ESCP_NamerFactionDunmerHouse</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->Great House [FamilyList]</li>
      </rulesStrings>
      <rulesFiles>
        <li>FamilyList->ESCP_DunmerNames/DunmerFamily</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ESCP_NamerSettlementDunmerHouse</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[DunmerCities]</li>
        <li>r_name(p=0.1)->Shrine of [DunmerGods]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>DunmerCities->ESCP_DunmerNames/DunmerCities</li>
		<li>DunmerGods->ESCP_DunmerNames/DunmerGods</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>ESCP_NamerSettlementTelvanni</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[TelvanniCities]</li>
        <li>r_name->Tel [FamilyList]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>TelvanniCities->ESCP_DunmerNames/TelvanniCities</li>
		<li>FamilyList->ESCP_DunmerNames/DunmerFamily</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <!-- Reavers -->
  
  <RulePackDef>
    <defName>ESCP_NamerFactionDunmerReaver</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->The [DunmerCities] Reavers</li>
        <li>r_name->[DunmerCities] Reavers</li>
        <li>r_name->The [FamilyList] Reavers</li>
        <li>r_name->[FamilyList]'s Reavers</li>
        <li>r_name->[DunmerGods]'s Reavers</li>
      </rulesStrings>
      <rulesFiles>
        <li>FamilyList->ESCP_DunmerNames/DunmerFamily</li>
		<li>DunmerCities->ESCP_DunmerNames/DunmerCities</li>
		<li>DunmerGods->ESCP_DunmerNames/DunmerGods</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

</Defs>
