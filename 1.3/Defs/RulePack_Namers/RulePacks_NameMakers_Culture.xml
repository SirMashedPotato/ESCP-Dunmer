<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>ESCP_LeaderTitleMaker_Ashlander</defName>
		<include>
		  <li>LeaderTitleMaker_GlobalStructure</li>
		</include>
		<rulePack>
		  <rulesStrings>
			<li>memeLeaderNoun(priority=1)->[cultureLeaderNoun]</li> <!-- ignore all meme nouns, tribals always use a culture noun -->

			<li>cultureLeaderTitle->[cultureLeaderAdjective][cultureLeaderNoun]</li>

			<li>cultureLeaderAdjective->ash</li>
			<li>cultureLeaderAdjective->great</li>
			<li>cultureLeaderAdjective->first</li>

			<li>cultureLeaderNoun->khan</li>
		  </rulesStrings>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
		<defName>ESCP_NamerDeity_Ashlander</defName>
		<include>
		  <li>NamerDeityGlobal</li>
		</include>
		<rulePack>
		  <rulesStrings>
			<li>name->[AshlanderGods]</li>
		  </rulesStrings>
		  <rulesFiles>
			<li>AshlanderGods->ESCP_AshlanderNames/AshlanderGods</li>
		  </rulesFiles>
		</rulePack>
	</RulePackDef>

	<!-- Dunmer -->

  <RulePackDef>
    <defName>ESCP_LeaderTitleMaker_Dunmeri</defName>
    <include>
      <li>LeaderTitleMaker_GlobalStructure</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>memeLeaderNoun(priority=1)->[cultureLeaderNoun]</li> <!-- ignore all meme nouns, tribals always use a culture noun -->

        <li>cultureLeaderTitle->[cultureLeaderAdjective] [cultureLeaderNoun]</li>

        <li>cultureLeaderAdjective->great</li>
        <li>cultureLeaderAdjective->high</li>
        <li>cultureLeaderAdjective->first</li>
        <li>cultureLeaderAdjective->grand</li>
        <li>cultureLeaderAdjective->arch</li>

        <li>cultureLeaderNoun->master</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>ESCP_NamerDeity_Dunmeri</defName>
	<include>
      <li>NamerDeityGlobal</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>name->[DunmerGods]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>DunmerGods->ESCP_DunmerNames/DunmerGods</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

</Defs>
