<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>ESCP_NamerPersonDunmerMale</defName>
		<rulePack>
			<rulesStrings>
				<li>name->[MaleList] [FamilyList]</li>
				<li>name(p=0.05)->[MaleList] 'the [MorrowindAnimals] of [DunmerCities]' [FamilyList]</li>
				<li>name(p=0.01)->[MaleList] 'chosen of [DunmerGods]' [FamilyList]</li>
				<li>name(p=0.0001)->[MaleList] 'fast Eddie' [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>MaleList</keyword>
					<path>ESCP_DunmerNames/DunmerMale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>ESCP_DunmerNames/DunmerFamily</path>
				</li>
			</rulesRaw>
			<rulesFiles>
				<li>MorrowindAnimals->ESCP_MiscMorrowindNames/MorrowindAnimals</li>
				<li>DunmerCities->ESCP_DunmerNames/DunmerCities</li>
				<li>DunmerGods->ESCP_DunmerNames/DunmerGods</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>

