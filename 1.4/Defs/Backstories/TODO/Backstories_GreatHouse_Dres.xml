<Defs>

	<AlienRace.AlienBackstoryDef>
		<defName>ESCP_Dunmer_DresBackstory</defName>
		<title>house Dres member</title>
		<titleShort>Dres member</titleShort>
		<baseDesc>[PAWN_nameDef] was a member of the Great House Dres, a traditionalist house famous for its slave plantations. Any Argonian in House Dres territory was a slave, and [PAWN_nameDef] liked it that way.</baseDesc>
		<slot>Adulthood</slot>
		<bodyTypeMale>Male</bodyTypeMale>
		<bodyTypeFemale>Female</bodyTypeFemale>
		<spawnCategories>
		  <li>ESCP_DunmerBackstory</li>
		  <li>ESCP_DunmerDresBackstory</li>
		</spawnCategories>
		<skillGains>
			<li>
				<key>Melee</key>
				<value>5</value>
			</li>
			<li>
				<key>Social</key>
				<value>2</value>
			</li>
		</skillGains>
		<forcedTraitsChance>
			<li>
				<defName>ESCP_Slaver</defName>
				<chance>100</chance>
			</li>
			<li>
				<defName>Xenophobia</defName>
				<degree>1</degree>
				<chance>100</chance>
			</li>
		</forcedTraitsChance>
		<disallowedTraitsChance>
			<li>
				<defName>Greedy</defName>
			</li>
			<li>
				<defName>Jealous</defName>
			</li>
		</disallowedTraitsChance>
		<workDisables>
			<li>Mining</li>
			<li>PlantWork</li>
		</workDisables>
	</AlienRace.AlienBackstoryDef>
	
