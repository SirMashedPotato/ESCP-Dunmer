	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2Base]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase" Name="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2Base" Abstract="True">
		<maleCommonality>50</maleCommonality>
		<femaleCommonality>50</femaleCommonality>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT3Base]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase" Name="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT3Base" Abstract="True">
		<maleCommonality>30</maleCommonality>
		<femaleCommonality>30</femaleCommonality>
	</AlienRace.AlienBackstoryDef>
	<!-- ==================== [Tier 1] ==================== -->
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT1A]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT1A</defName>
		<title>Hlaalu member</title>
		<titleShort>member</titleShort>
		<baseDesc>[PAWN_nameDef] was a fairly unremarkable member of Great House Hlaalu. Members of this Great are notorious for valuing wealth above all else, even morals. [PAWN_nameDef] once thought [PAWN_pronoun] was different, now [PAWN_pronoun] is not so sure.</baseDesc>
		<bodyTypeMale>Male</bodyTypeMale>
		<bodyTypeFemale>Female</bodyTypeFemale>
		<skillGains>
			<li>
				<key>Melee</key>
				<value>2</value>
			</li>
			<li>
				<key>Social</key>
				<value>5</value>
			</li>
		</skillGains>
		<workDisables>
			<li>ManualDumb</li>
		</workDisables>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT1B]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT1B</defName>
		<title>Hlaalu thug</title>
		<titleShort>thug</titleShort>
		<baseDesc>House Hlaalu made a great number of transactions each day, many of which were somewhat less-than-legal. [PAWN_nameDef] ensured these transactions always went as planned.</baseDesc>
		<bodyTypeMale>Male</bodyTypeMale>
		<bodyTypeFemale>Female</bodyTypeFemale>
		<skillGains>
			<li>
				<key>Melee</key>
				<value>3</value>
			</li>
			<li>
				<key>Social</key>
				<value>3</value>
			</li>
		</skillGains>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT1C]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT1C</defName>
		<title>Hlaalu envoy</title>
		<titleShort>envoy</titleShort>
		<baseDesc>[PAWN_nameDef] was one of the many envoys that Hlaalu would send to talk things out with people. While [PAWN_pronoun] is rather good with words, sometimes words just aren't enough. That's what the 'bodyguards' sent with [PAWN_nameDef] were for.</baseDesc>
		<bodyTypeMale>Male</bodyTypeMale>
		<bodyTypeFemale>Female</bodyTypeFemale>
		<skillGains>
			<li>
				<key>Intellectual</key>
				<value>2</value>
			</li>
			<li>
				<key>Social</key>
				<value>4</value>
			</li>
		</skillGains>
	</AlienRace.AlienBackstoryDef>
	<!-- ==================== [Tier 2] ==================== -->
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2A]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2A</defName>
		<title>Hlaalu merchant</title>
		<titleShort>merchant</titleShort>
		<baseDesc>It was well known that merchants operating in House Hlaalu territory were very well off, and [PAWN_nameDef] took great advantage of this. [PAWN_pronoun] also took great advantage of House Hlaalu's connections with a certain criminal Tong.</baseDesc>
		<bodyTypeGlobal>Fat</bodyTypeGlobal>
		<skillGains>
			<li>
				<key>Social</key>
				<value>9</value>
			</li>
		</skillGains>
		<workDisables>
			<li>ManualDumb</li>
		</workDisables>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2B]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2B</defName>
		<title>Hlaalu enforcer</title>
		<titleShort>enforcer</titleShort>
		<baseDesc>Whenever the thugs failed, [PAWN_nameDef] came knocking. Whether it be on a door, or on a head, the transactions were always completed.</baseDesc>
		<bodyTypeGlobal>Hulk</bodyTypeGlobal>
		<skillGains>
			<li>
				<key>Melee</key>
				<value>9</value>
			</li>
		</skillGains>
		<workDisables>
			<li>Social</li>
		</workDisables>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2C]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2C</defName>
		<title>Hlaalu arsonist</title>
		<titleShort>arsonist</titleShort>
		<baseDesc>[PAWN_nameDef] was rather skilled at starting fires, and very much enjoyed the sight of the flames flickering through someones house. Sometimes higher ranking members of Great House Hlaalu needed evidence to disappear, when that need arised, they would contact someone like [PAWN_nameDef].</baseDesc>
		<bodyTypeGlobal>Thin</bodyTypeGlobal>
		<skillGains>
			<li>
				<key>Crafting</key>
				<value>2</value>
			</li>
			<li>
				<key>Shooting</key>
				<value>3</value>
			</li>
			<li>
				<key>Social</key>
				<value>3</value>
			</li>
		</skillGains>
		<forcedTraitsChance>
			<li>
				<defName>Pyromaniac</defName>
				<chance>100</chance>
			</li>
		</forcedTraitsChance>
		<workDisables>
			<li>Firefighting</li>
		</workDisables>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2D]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2D</defName>
		<title>Hlaalu foreman</title>
		<titleShort>foreman</titleShort>
		<baseDesc>[PAWN_nameDef] was one of the foreman in charge of overseeing the construction, and renovation, of strongholds in Hlaalu land. [PAWN_pronoun] made sure no expenses were spared, by keeping most of the coin safe in [PAWN_possessive] personal vault. Indefinitely.</baseDesc>
		<bodyTypeGlobal>Fat</bodyTypeGlobal>
		<skillGains>
			<li>
				<key>Construction</key>
				<value>4</value>
			</li>
			<li>
				<key>Social</key>
				<value>5</value>
			</li>
		</skillGains>
		<workDisables>
			<li>ManualDumb</li>
		</workDisables>
	</AlienRace.AlienBackstoryDef>
	<!-- ==========[ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2E]========== -->
	<AlienRace.AlienBackstoryDef ParentName="ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluBase">
		<defName>ESCP_DunmerBackstory_Adulthood_GreatHouseHlaaluT2E</defName>
		<title>Hlaalu torturer</title>
		<titleShort>torturer</titleShort>
		<baseDesc>After spending years training in the arts of healing, [PAWN_nameDef] eventually discovered this knowledge could be used for more sinister purposes. [PAWN_pronoun] was rather fond of incorporating netch barbs into the process.</baseDesc>
		<bodyTypeGlobal>Thin</bodyTypeGlobal>
		<skillGains>
			<li>
				<key>Animals</key>
				<value>2</value>
			</li>
			<li>
				<key>Intellectual</key>
				<value>2</value>
			</li>
			<li>
				<key>Medicine</key>
				<value>3</value>
			</li>
		</skillGains>
		<workDisables>
			<li>ManualDumb</li>
		</workDisables>
	</AlienRace.AlienBackstoryDef>
	<!-- ==================== [Tier 3] ==================== -->
	<!-- ==================== [Special] ==================== -->
</Defs>
