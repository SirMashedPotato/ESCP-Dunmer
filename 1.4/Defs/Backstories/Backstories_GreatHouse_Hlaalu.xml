		<spawnCategories>
			<li>ESCP_DunmerGreatHouse_Hlaalu_Adulthood</li>
		</spawnCategories>
		<forcedTraitsChance>
			<li>
				<defName>Greedy</defName>
				<chance>25</chance>
			</li>
			<li>
				<defName>Psychopath</defName>
				<chance>15</chance>
			</li>
		</forcedTraitsChance>
		<disallowedTraitsChance>
			<li>
				<defName>Ascetic</defName>
			</li>
		</disallowedTraitsChance>
		<maleCommonality>80</maleCommonality>
		<femaleCommonality>80</femaleCommonality>
