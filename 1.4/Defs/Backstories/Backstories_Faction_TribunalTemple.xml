			</li>
			<li>
				<defName>Nerves</defName>
				<degree>1</degree>
				<chance>100</chance>
			</li>
		</forcedTraits>
		<forcedHediffs>
		</forcedHediffs>
		<maleCommonality>5</maleCommonality>
		<femaleCommonality>5</femaleCommonality>
