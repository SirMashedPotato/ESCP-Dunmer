		  <li>ESCP_TribunalBackstory</li>
		</spawnCategories>
		<skillGains>
			<li>
				<defName>Melee</defName>
				<amount>8</amount>
			</li>
			<li>
				<defName>Shooting</defName>
				<amount>2</amount>
			</li>
		</skillGains>
		<forcedTraits>
